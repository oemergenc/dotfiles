# auto complete an alias in zsh Ctrl-x a
# auto complete an alias in bash Ctrl-Alt e

alias pbcopy='xclip -selection clipboard'
alias uzshrc="source ~/.zshrc"
alias uz=uzshrc
alias ezshrc="vim ~/.zshrc"
alias ez=ezshrc
alias ealiases="vim $HOME/.dotfiles/aliases"
alias ea=ealiases

source $HOME/.dotfiles/alias/aliases_git.sh